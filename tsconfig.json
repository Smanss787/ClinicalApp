{
  "compilerOptions": {
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "jsx": "react-native",
    "lib": ["es2017"],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"],
      "@modules/*": ["src/modules/*"]
    }
  },
  "exclude": ["node_modules"]  
}
